    "service-report": {
      "type": "application",
      "root": "apps/service-report",
      "entryFile": "main",
      "sourceRoot": "apps/service-report/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-report/tsconfig.app.json"
      }
    },
    "service-payment-gateway": {
      "type": "application",
      "root": "apps/service-payment-gateway",
