      "root": "libs/plugins",
      "entryFile": "index",
      "sourceRoot": "libs/plugins/src",
      "compilerOptions": {
        "tsConfigPath": "libs/plugins/tsconfig.lib.json"
      }
    },
    "service-auth": {
      "type": "application",
      "root": "apps/service-auth",
      "entryFile": "main",
      "sourceRoot": "apps/service-auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-auth/tsconfig.app.json"
      }
    },
    "service-bill": {
      "type": "application",
      "root": "apps/service-bill",
      "entryFile": "main",
      "sourceRoot": "apps/service-bill/src",
